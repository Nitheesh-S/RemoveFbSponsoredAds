{
 "name": "RemoveSponsored",
 "description": "Remove Facebook Sponsored",
 "icons": {
     "16": "icon-16.png",
     "48": "icon-48.png",
     "128": "icon-128.png" },
 "version": "0.1",
 "manifest_version": 2,
 "content_scripts": [
    {
        "matches": [
            "*://*.facebook.com/*"
        ],
        "js": [
            "content.js"
        ]
    }
 ]
}